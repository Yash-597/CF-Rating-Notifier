{
  "manifest_version": 3,
  "name": "Codeforces Rating Notifier",
  "version": "1.0",
  "description": "Get notified when your Codeforces rating updates.",
  "permissions": ["alarms", "notifications", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "icon.png"
  },
  "action": {
    "default_icon": "icon.png",
    "default_title": "CF Notifier",
    "default_popup": "index.html"
  },
  "options_page": "index.html"
}
